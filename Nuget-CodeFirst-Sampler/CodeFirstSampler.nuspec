<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>SampleCodeFirstCodes</id>
    <version>$version$</version>
    <authors>Kadir Murat Başeren</authors>
    <title>Sample CodeFirst Codes</title>
    <owners>Kadir Murat Başeren</owners>
    <projectUrl>https://github.com/muratbaseren/Nuget-CodeFirst-Sampler</projectUrl>
    <iconUrl>https://muratbaseren.files.wordpress.com/2017/02/nuget-icon-sample-codefirst-app.png</iconUrl>
    <description>This package includes sample controllers, models, views for asp.net mvc codefirst project. Also it will generate sample seed data for database.</description>
    <summary>Sample CodeFirst Project that includes Customers, Products, Categories Controllers and their views.</summary>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <tags>aspnet,mvc,codefirst,entityframework,sample codes,automapper,bootstrap,jquery,fakedata,migration</tags>
    <copyright>Copyright Kadir Murat Başeren</copyright>
    <dependencies>
      <group>
        <dependency id="EntityFramework" />
        <dependency id="jQuery" />
        <dependency id="bootstrap" />
        <dependency id="FakeData" />
        <dependency id="FontAwesome" />
        <dependency id="AutoMapper" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="Assets\**\*.*" target="content\Assets" />
    <file src="Context\**\*.*" target="content\Context" />
    <!--<file src="Content\**\*.*" target="content\Content" />-->
    <!--<file src="Scripts\**\*.*" target="content\Scripts" />-->
    <file src="Controllers\**\*.*" target="content\Controllers" />
    <file src="Models\**\*.*" target="content\Models" />
    <file src="Views\**\*.*" target="content\Views" />
    <file src="web.config.install.xdt" target="content\web.config.install.xdt" />
    <file src="web.config.uninstall.xdt" target="content\web.config.uninstall.xdt" />
  </files>
</package>

